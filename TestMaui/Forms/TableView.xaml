<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TestMaui.Forms.TableView"
             xmlns:local="clr-namespace:TestMaui.Extensions"
             Title="TableView">
    <TableView Intent="Form">
        <TableView.Root>
            <TableRoot>
                <TableSection Title="Basics">
                    <TextCell Text="Name" Detail="Khush"></TextCell>
                    <EntryCell Label="Title" Placeholder="Test" Keyboard="Numeric"></EntryCell>
                    <SwitchCell Text="Notifications" On="True" OnChanged="SwitchCell_OnChanged">
                    </SwitchCell>
                    <local:DateCell Label="Starts">
                        
                    </local:DateCell>
                    <local:DateCell Label="Ends">

                    </local:DateCell>
                    
                </TableSection>
            </TableRoot>
        </TableView.Root>
    </TableView>
</ContentPage>